
@{
    ViewData["Title"] = "addstatic";
}
@model Turbo.ViewModel.StaticDbData
<h4 style="margin-bottom:50px;">Elave edeceyiniz datan novunu mueyyenlesdirib datani elave edin.</h4>

<div class="container">
    <div class="row">
        <div class="col-12">
            <form method="post">
                <div class="text-danger" asp-validation-summary="ModelOnly">

                </div>
                <div class="col-8">

                    <div class="form-check">
                        <input class="form-check-input" type="radio" value="marka" asp-for="StaticDataName" checked>
                        <label class="form-check-label" asp-for="StaticDataName">
                            Masin markasi daxil edin
                        </label>
                    </div>
                    <div class="form-check">
                        <input class="form-check-input" type="radio" value="reng" asp-for="StaticDataName">
                        <label class="form-check-label" asp-for="StaticDataName">
                            Reng daxil edin
                        </label>
                    </div>
                    <div class="form-check">
                        <input class="form-check-input" type="radio" value="seher" asp-for="StaticDataName">
                        <label class="form-check-label" asp-for="StaticDataName">
                          Seher daxil edin
                        </label>
                    </div>

                    <div class="form-group">
                        <label asp-for="Name"></label>
                        <input asp-for="Name" class="form-control" placeholder="Ad daxil edin">
                        <span asp-validation-for="Name"></span>
                    </div>
                </div>
                
                <div class="col-12"><input class="btn btn-primary" type="submit" value="Yarat" /></div>

            </form>
        </div>
    </div>
</div>

