
@{
    ViewData["Title"] = "Signup";
}

@model Turbo.ViewModel.SignUp
    
<div class="container">
    <div class="row">
        <div class="col-6" style="margin-bottom:50px">
            <form class="mt-5" method="post">
                <div class="text-danger" asp-validation-summary="ModelOnly">

                </div>
                <div class="form-group">
                    <label asp-for="Ad"></label>
                    <input asp-for="Ad" class="form-control" placeholder="Ad">
                    <span asp-validation-for="Ad"></span>
                </div>
                <div class="form-group">
                    <label asp-for="Soyad"></label>
                    <input asp-for="Soyad" class="form-control" placeholder="Soyad">
                    <span asp-validation-for="Soyad"></span>
                </div>
                <div class="form-group">
                    <label asp-for="Email"></label>
                    <input asp-for="Email" class="form-control" placeholder="Email">
                    <span asp-validation-for="Email"></span>
                </div>
                <div class="form-group">
                    <label asp-for="Username"></label>
                    <input asp-for="Username" class="form-control" placeholder="Username">
                    <span asp-validation-for="Username"></span>
                </div>
                <div class="form-group">
                    <label asp-for="Telofon"></label>
                    <input asp-for="Telofon" class="form-control" placeholder="Telofon">
                    <span asp-validation-for="Telofon"></span>
                </div>




                <div class="form-group">
                    <label asp-for="Password"></label>
                    <input asp-for="Password" class="form-control" placeholder="Password">
                    <span asp-validation-for="Password"></span>
                </div>
                <div class="form-group">
                    <label asp-for="ConiformPassword"></label>
                    <input asp-for="ConiformPassword" class="form-control" placeholder="Coniform PassWord">
                    <span asp-validation-for="ConiformPassword"></span>
                </div>
                <button type="submit" class="btn btn-primary">Qeyd Ol</button>
            </form>
        </div>
    </div>
</div>

    

@section Scripts{
    
    <partial name="_ValidationScriptsPartial" />
}






